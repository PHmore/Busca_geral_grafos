import pygraphviz as pgv

# Crie um novo grafo direcionado usando PyGraphviz
G_pvg = pgv.AGraph(strict=False, directed=True)

# Adicione alguns nós ao grafo
G_pvg.add_node('A')
G_pvg.add_node('B')
G_pvg.add_node('C')
G_pvg.add_node('D')

# Obtenha todos os nós do grafo
nodes_list = G_pvg.nodes()

# Exiba a lista de nós
print(nodes_list)
print(len(nodes_list))